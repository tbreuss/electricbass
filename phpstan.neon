includes:
	- vendor/proget-hq/phpstan-yii2/extension.neon
parameters:
	level: 5
	bootstrapFiles:
	    - .phpstan/constants.php
	scanFiles:
	    - vendor/yiisoft/yii2/Yii.php
	stubFiles:
	    - .phpstan/stubs/View.stub
	paths:
		- assets
		- commands
		- components
		- config
		- controllers
		- filters
		- helpers
		- mail
		- messages
		- migrations
		- models
		- modules
		#- tests
		- traits
		- views
		- web
		- widgets
	excludePaths:
	    analyse:
	        - components/AmazonProductDetail.php
	yii2:
	    config_path: %rootDir%/../../../config/test.php
	ignoreErrors:
	    - '#Call to an undefined method yii\\base\\View::registerLinkTag#'
	    - '#Call to an undefined method yii\\base\\Component::getRoute#'
	    - '#Access to an undefined property yii\\base\\Application::\$shortcode#'
